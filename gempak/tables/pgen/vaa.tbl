!
! Identify which VAA form entries are used to make a VAA text product.
! Value after VAA form entry is default.
! LOC_1 refers to availability on the "VAA - (volcano name)" form,
! LOC_2 refers to availability on the "VAA Create" form.
!
!	LOCATION PRODUCT <VAA form entry>Value
!
!---------------------------------------------------------------------
!
! LOC_1 entries -- available on the "VAA - (volcano name)" form
!
FORMAT  LOC_1  NORMAL      <VOLCANO>
FORMAT  LOC_1  NORMAL      <NUMBER>
FORMAT  LOC_1  NORMAL      <LOCATION>
FORMAT  LOC_1  NORMAL      <AREA>
FORMAT  LOC_1  NORMAL      <SUMMIT ELEVATION>
FORMAT  LOC_1  NORMAL      <ADVISORY NUMBER>
FORMAT  LOC_1  NORMAL      <INFORMATION SOURCE>
FORMAT  LOC_1  NORMAL      <ERUPTION DETAILS>
FORMAT  LOC_1  NORMAL      <OBS ASH DATE/TIME>
FORMAT  LOC_1  NORMAL      <OBS ASH CLOUD>
FORMAT  LOC_1  NORMAL      <FCST ASH CLOUD +6H>
FORMAT  LOC_1  NORMAL      <FCST ASH CLOUD +12H>
FORMAT  LOC_1  NORMAL      <FCST ASH CLOUD +18H>
FORMAT  LOC_1  NORMAL      <REMARKS>
FORMAT  LOC_1  NORMAL      <NEXT ADVISORY>
!
FORMAT  LOC_1  END         <VOLCANO>
FORMAT  LOC_1  END         <NUMBER>
FORMAT  LOC_1  END         <LOCATION>
FORMAT  LOC_1  END         <AREA>
FORMAT  LOC_1  END         <SUMMIT ELEVATION>
FORMAT  LOC_1  END         <ADVISORY NUMBER>
FORMAT  LOC_1  END         <INFORMATION SOURCE>
FORMAT  LOC_1  END         <ERUPTION DETAILS>
FORMAT  LOC_1  END         <OBS ASH DATE/TIME>
FORMAT  LOC_1  END         <OBS ASH CLOUD>VA NOT IDENTIFIABLE FROM SATELLITE DATA.
FORMAT  LOC_1  END         <FCST ASH CLOUD +6H>
FORMAT  LOC_1  END         <FCST ASH CLOUD +12H>
FORMAT  LOC_1  END         <FCST ASH CLOUD +18H>
FORMAT  LOC_1  END         <NEXT ADVISORY> NO FURTHER ADVISORIES
!
FORMAT  LOC_1  QUICK       <VOLCANO>
FORMAT  LOC_1  QUICK       <NUMBER>
FORMAT  LOC_1  QUICK       <LOCATION>
FORMAT  LOC_1  QUICK       <AREA>
FORMAT  LOC_1  QUICK       <SUMMIT ELEVATION>
FORMAT  LOC_1  QUICK       <ADVISORY NUMBER>
FORMAT  LOC_1  QUICK       <INFORMATION SOURCE>
FORMAT  LOC_1  QUICK       <ERUPTION DETAILS>
FORMAT  LOC_1  QUICK       <OBS ASH DATE/TIME>
FORMAT  LOC_1  QUICK       <OBS ASH CLOUD>
FORMAT  LOC_1  QUICK       <FCST ASH CLOUD +6H>
FORMAT  LOC_1  QUICK       <FCST ASH CLOUD +12H>
FORMAT  LOC_1  QUICK       <FCST ASH CLOUD +18H>
FORMAT  LOC_1  QUICK       <REMARKS>WE HAVE RECEIVED INFORMATION SUGGESTING A POSSIBLE VA EMISSION. WE WILL GATHER FURTHER INFORMATION AND ISSUE A FULL ADVISORY AS SOON AS POSSIBLE.
FORMAT  LOC_1  QUICK       <NEXT ADVISORY>AS SOON AS POSSIBLE
!
FORMAT  LOC_1  NEAR        <VOLCANO> 
FORMAT  LOC_1  NEAR        <LOCATION>
FORMAT  LOC_1  NEAR        <AREA>
FORMAT  LOC_1  NEAR        <SUMMIT ELEVATION>
FORMAT  LOC_1  NEAR        <ADVISORY NUMBER>
FORMAT  LOC_1  NEAR        <INFORMATION SOURCE>NOT PROVIDED
FORMAT  LOC_1  NEAR        <ERUPTION DETAILS>NOT PROVIDED
FORMAT  LOC_1  NEAR        <OBS ASH DATE/TIME>NOT PROVIDED
FORMAT  LOC_1  NEAR        <OBS ASH CLOUD>NOT PROVIDED
FORMAT  LOC_1  NEAR        <FCST ASH CLOUD +6H>NOT PROVIDED
FORMAT  LOC_1  NEAR        <FCST ASH CLOUD +12H>NOT PROVIDED
FORMAT  LOC_1  NEAR        <FCST ASH CLOUD +18H>NOT PROVIDED
FORMAT  LOC_1  NEAR        <REMARKS>
FORMAT  LOC_1  NEAR        <NEXT ADVISORY>
!
FORMAT  LOC_1  WATCH       <VOLCANO> 
FORMAT  LOC_1  WATCH       <NUMBER>
FORMAT  LOC_1  WATCH       <LOCATION>
FORMAT  LOC_1  WATCH       <AREA>
FORMAT  LOC_1  WATCH       <SUMMIT ELEVATION>
FORMAT  LOC_1  WATCH       <ADVISORY NUMBER>
FORMAT  LOC_1  WATCH       <INFORMATION SOURCE>
FORMAT  LOC_1  WATCH       <ERUPTION DETAILS>NO ERUPTION HAS OCCURRED
FORMAT  LOC_1  WATCH       <REMARKS> 
FORMAT  LOC_1  WATCH       <NEXT ADVISORY>WILL BE ISSUED DAILY UNTIL THE IMMINENT THREAT HAS PASSED OR SOONER IF AN ERUPTION OCCURS.
!
FORMAT  LOC_1  TEST        <STATUS>TEST
FORMAT  LOC_1  TEST        <VOLCANO> 
FORMAT  LOC_1  TEST        <LOCATION>
FORMAT  LOC_1  TEST        <AREA>
FORMAT  LOC_1  TEST        <SUMMIT ELEVATION>
FORMAT  LOC_1  TEST        <ADVISORY NUMBER>
FORMAT  LOC_1  TEST        <INFORMATION SOURCE>TEST ... TEST
FORMAT  LOC_1  TEST        <ERUPTION DETAILS>TEST - NO ERUPTION
FORMAT  LOC_1  TEST        <OBS ASH DATE/TIME>
FORMAT  LOC_1  TEST        <OBS ASH CLOUD>
FORMAT  LOC_1  TEST        <FCST ASH CLOUD +6H>
FORMAT  LOC_1  TEST        <FCST ASH CLOUD +12H>
FORMAT  LOC_1  TEST        <FCST ASH CLOUD +18H>
FORMAT  LOC_1  TEST        <REMARKS>TEST NO ERUPTION TEST TEST.
FORMAT  LOC_1  TEST        <NEXT ADVISORY>
!
FORMAT  LOC_1  EXER        <STATUS>EXER
FORMAT  LOC_1  EXER        <VOLCANO> 
FORMAT  LOC_1  EXER        <LOCATION>
FORMAT  LOC_1  EXER        <AREA>
FORMAT  LOC_1  EXER        <SUMMIT ELEVATION>
FORMAT  LOC_1  EXER        <ADVISORY NUMBER>
FORMAT  LOC_1  EXER        <INFORMATION SOURCE>EXERCISE ... EXERCISE
FORMAT  LOC_1  EXER        <ERUPTION DETAILS>EXERCISE - NO ERUPTION
FORMAT  LOC_1  EXER        <OBS ASH DATE/TIME>
FORMAT  LOC_1  EXER        <OBS ASH CLOUD>
FORMAT  LOC_1  EXER        <FCST ASH CLOUD +6H>
FORMAT  LOC_1  EXER        <FCST ASH CLOUD +12H>
FORMAT  LOC_1  EXER        <FCST ASH CLOUD +18H>
FORMAT  LOC_1  EXER        <REMARKS>EXERCISE NO ERUPTION EXERCISE EXERCISE.
FORMAT  LOC_1  EXER        <NEXT ADVISORY>
!
!---------------------------------------------------------------------
!
! LOC_2 emtroes -- available on the "VAA Create" form.
!
FORMAT  LOC_2  TEST        <VOLCANO>TEST
FORMAT  LOC_2  TEST        <NUMBER>9999-99
FORMAT  LOC_2  TEST        <LOCATION>N9999W99999
FORMAT  LOC_2  TEST        <AREA>TEST
FORMAT  LOC_2  TEST        <SUMMIT ELEVATION>9999 FT (9999M)
FORMAT  LOC_2  TEST        <ADVISORY NUMBER>001
FORMAT  LOC_2  TEST        <REMARKS>TEST...TEST...TEST. THIS IS ONLY A TEST.
!
FORMAT  LOC_2  RESUME      <VOLCANO>USER MESSAGE
FORMAT  LOC_2  RESUME      <LOCATION>XX
FORMAT  LOC_2  RESUME      <AREA>XX
FORMAT  LOC_2  RESUME      <REMARKS>THE WASHINGTON VAAC HAS RESUMED NORMAL OPERATIONS. 
!
FORMAT  LOC_2  BACKUP      <VOLCANO> USER MESSAGE
FORMAT  LOC_2  BACKUP      <LOCATION>XX
FORMAT  LOC_2  BACKUP      <AREA>XX
FORMAT  LOC_2  BACKUP      <REMARKS>  

